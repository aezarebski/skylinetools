<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beast beautitemplate='Standard' beautistatus='' 
       namespace="beast.core
                 :beast.evolution.alignment
                 :beast.evolution.tree.coalescent
                 :beast.core.util
                 :beast.evolution.nuc
                 :beast.evolution.operators
                 :beast.evolution.sitemodel
                 :beast.evolution.substitutionmodel
                 :beast.evolution.likelihood
                 :skylinetools.util" 
       required="BDSKY v1.3.3" version="2.4">


<!--  

      Dengue example with reproductiveNumber shift dates shared over two trees.
      - Tree C1: 3 change-point times with dates equally spaced between tMRCA and most-recent sample (1984) 
      - Tree C2: Same 3 change-point dates as Tree C1
      
      Both trees share the same HKY model and clock model (Kappy, frequencies and clock rate)
      Both trees have the same becoming noninfectious rate
      Each tree has its own reproductive number, sampling proportion and origin

      Possible modification: 
            Give each alignment/tree its own mutation rate and fix the mean mutation rate to 1
            (Gives some freedom for different clusters to evolve at different speeds)

-->



<map name="Uniform" >beast.math.distributions.Uniform</map>
<map name="Exponential" >beast.math.distributions.Exponential</map>
<map name="LogNormal" >beast.math.distributions.LogNormalDistributionModel</map>
<map name="Normal" >beast.math.distributions.Normal</map>
<map name="Beta" >beast.math.distributions.Beta</map>
<map name="Gamma" >beast.math.distributions.Gamma</map>
<map name="LaplaceDistribution" >beast.math.distributions.LaplaceDistribution</map>
<map name="prior" >beast.math.distributions.Prior</map>
<map name="InverseGamma" >beast.math.distributions.InverseGamma</map>
<map name="OneOnX" >beast.math.distributions.OneOnX</map>


<data id="Dengue4.C1" name="alignment">
      <sequence id="seq_D4Philip56" taxon="D4Philip56" totalcount="4" value=""/>
      <sequence id="seq_D4Philip64" taxon="D4Philip64" totalcount="4" value=""/>
      <sequence id="seq_D4Philip84" taxon="D4Philip84" totalcount="4" value=""/>
      <sequence id="seq_D4SLanka78" taxon="D4SLanka78" totalcount="4" value=""/>
      <sequence id="seq_D4Thai63" taxon="D4Thai63" totalcount="4" value=""/>
      <sequence id="seq_D4Thai78" taxon="D4Thai78" totalcount="4" value=""/>
      <sequence id="seq_D4Thai84" taxon="D4Thai84" totalcount="4" value=""/>
</data>

<data id="Dengue4.C2" name="alignment">
      <sequence id="seq_D4Brazi82" taxon="D4Brazi82" totalcount="4" value=""/>
      <sequence id="seq_D4ElSal83" taxon="D4ElSal83" totalcount="4" value=""/>
      <sequence id="seq_D4ElSal94" taxon="D4ElSal94" totalcount="4" value=""/>
      <sequence id="seq_D4Indon76" taxon="D4Indon76" totalcount="4" value=""/>
      <sequence id="seq_D4Indon77" taxon="D4Indon77" totalcount="4" value=""/>
      <sequence id="seq_D4Mexico84" taxon="D4Mexico84" totalcount="4" value=""/>
      <sequence id="seq_D4NewCal81" taxon="D4NewCal81" totalcount="4" value=""/>
      <sequence id="seq_D4PRico86" taxon="D4PRico86" totalcount="4" value=""/>
      <sequence id="seq_D4Tahiti79" taxon="D4Tahiti79" totalcount="4" value=""/>
      <sequence id="seq_D4Tahiti85" taxon="D4Tahiti85" totalcount="4" value=""/>
</data>


<!-- Convert heights to dates --> 
<!--RPNcalculator id="ReSliceDates.C1.RPN" spec="beast.math.statistic.RPNcalculator" expression="1984 ReTreeSlice.C1 -">
      <parameter idref="ReTreeSlice.C1"/>
</RPNcalculator-->

<!-- Remove most recent date from the slice, since the most recent date in C1 is 1984, but in C2 is 1994 -->
<function spec="Slice" id="ReSliceDates.C1.trim" arg="@ReSliceDates.C1" index="1" count="2"/>
    
<run id="mcmc" spec="MCMC" chainLength="5000000">
      <state id="state" storeEvery="10000000">

            <tree id="Tree.C1" name="stateNode">
                  <trait id="dateTrait.C1" spec="beast.evolution.tree.TraitSet" traitname="date">
                        D4Philip64=1964,
                        D4Philip56=1956,
                        D4Philip84=1984,
                        D4SLanka78=1978,
                        D4Thai63=1963,
                        D4Thai78=1978,
                        D4Thai84=1984                
                        <taxa id="TaxonSet.Dengue4.C1" spec="TaxonSet">
                          <alignment idref="Dengue4.C1"/>
                        </taxa>
                  </trait>
                  <taxonset idref="TaxonSet.Dengue4.C1"/>
            </tree>

            <tree id="Tree.C2" name="stateNode">
                  <trait id="dateTrait.C2" spec="beast.evolution.tree.TraitSet" traitname="date">
                        D4Brazi82=1982,
                        D4ElSal83=1983,
                        D4ElSal94=1994,
                        D4Indon76=1976,
                        D4Indon77=1977,
                        D4Mexico84=1984,
                        D4NewCal81=1981,
                        D4PRico86=1986,
                        D4Tahiti79=1979,
                        D4Tahiti85=1985
                        <taxa id="TaxonSet.Dengue4.C2" spec="TaxonSet">
                          <alignment idref="Dengue4.C2"/>
                        </taxa>
                  </trait>
                  <taxonset idref="TaxonSet.Dengue4.C2"/>
            </tree>

            <!-- Parameters shared between both trees -->
            <parameter id="kappa" lower="0.0" name="stateNode">2.0</parameter>
            <parameter id="freqParameter" dimension="4" lower="0.0" name="stateNode" upper="1.0">0.25</parameter>
            <parameter id="clockRate" name="stateNode">1.0</parameter>

            <parameter id="becomeUninfectiousRate" lower="0.0" name="stateNode" upper="Infinity">1.0</parameter>

            <!-- Parameters specific to each tree -->
            <parameter id="origin.C1" lower="0.0" name="stateNode" upper="Infinity">100.0</parameter>
            <parameter id="samplingProportion.C1" lower="0.0" name="stateNode" upper="1.0">0.01</parameter>
            <parameter id="reproductiveNumber.C1" dimension="3" lower="0.0" name="stateNode" upper="Infinity">2.0</parameter>

            <parameter id="origin.C2" lower="0.0" name="stateNode" upper="Infinity">100.0</parameter>
            <parameter id="samplingProportion.C2" lower="0.0" name="stateNode" upper="1.0">0.01</parameter>
            <parameter id="reproductiveNumber.C2" dimension="3" lower="0.0" name="stateNode" upper="Infinity">2.0</parameter>
      </state>

      <!-- Initial trees -->
      <init id="RandomTree.C1" spec="beast.evolution.tree.RandomTree" estimate="false" initial="@Tree.C1" taxa="@Dengue4.C1">
            <populationModel spec="ConstantPopulation" popSize="1.0"/>
      </init>

      <init id="RandomTree.C2" spec="beast.evolution.tree.RandomTree" estimate="false" initial="@Tree.C2" taxa="@Dengue4.C2">
            <populationModel spec="ConstantPopulation" popSize="1.0"/>
      </init>


      <distribution id="posterior" spec="util.CompoundDistribution">

            <!-- Begin prior -->
            <distribution id="prior" spec="util.CompoundDistribution">

                  <!-- Treeprior C1 -->
                  <distribution id="BirthDeathSkySerial.C1" 
                                spec="beast.evolution.speciation.BirthDeathSkylineModel" 
                                tree="@Tree.C1"
                                reproductiveNumber="@reproductiveNumber.C1" 
                                becomeUninfectiousRate="@becomeUninfectiousRate" 
                                samplingProportion="@samplingProportion.C1" 
                                origin="@origin.C1">
                        <birthRateChangeTimes spec="TreeSlicer" id="ReTreeSlice.C1" tree="@Tree.C1" dimension="3" stop="tmrca" includeLast="false"/>
                        <reverseTimeArrays spec="beast.core.parameter.BooleanParameter" value="true false false false false"/>
                  </distribution>

                  <!-- Treeprior C2 -->
                  <distribution id="BirthDeathSkySerial.C2" 
                                spec="beast.evolution.speciation.BirthDeathSkylineModel" 
                                tree="@Tree.C2"
                                reproductiveNumber="@reproductiveNumber.C2" 
                                becomeUninfectiousRate="@becomeUninfectiousRate" 
                                samplingProportion="@samplingProportion.C2" 
                                origin="@origin.C2">
                        <birthRateChangeTimes spec="TreeSlicer" id="ReTreeSlice.C2" tree="@Tree.C2" dates="@ReSliceDates.C1.trim" type="dates"/>
                        <reverseTimeArrays spec="beast.core.parameter.BooleanParameter" value="true false false false false"/>
                  </distribution>

                  <!-- Treeprior priors -->
                  <prior id="becomeUninfectiousRatePrior" name="distribution" x="@becomeUninfectiousRate">
                        <LogNormal name="distr" M="0" S="1" offset="0.0" meanInRealSpace="false"/>
                  </prior>

                  <!-- C1 BDSKY priors -->
                        <prior id="reproductiveNumberPrior.C1" name="distribution" x="@reproductiveNumber.C1">                      
                              <LogNormal name="distr" M="0" S="1" offset="0.0"/>
                        </prior>                  
                        <prior id="samplingProportionPrior.C1" name="distribution" x="@samplingProportion.C1">                  
                              <Beta name="distr" alpha="2" beta="10"/>
                        </prior>
                        <prior id="originPrior.C1" name="distribution" x="@origin.C1">
                            <Uniform name="distr" upper="500" lower="0"/>
                        </prior>

                  <!-- C2 BDSKY priors -->
                        <prior id="reproductiveNumberPrior.C2" name="distribution" x="@reproductiveNumber.C2">                      
                              <LogNormal name="distr" M="0" S="1" offset="0.0"/>
                        </prior>                  
                        <prior id="samplingProportionPrior.C2" name="distribution" x="@samplingProportion.C2">                  
                              <Beta name="distr" alpha="2" beta="10"/>
                        </prior>
                        <prior id="originPrior.C2" name="distribution" x="@origin.C2">
                            <Uniform name="distr" upper="500" lower="0"/>
                        </prior>

                  <!-- Site model priors -->
                  <prior id="KappaPrior" name="distribution" x="@kappa">
                        <LogNormal name="distr" M="1" S="1.25" offset="0.0"/>
                  </prior>

                  <!-- Clock model priors -->
                  <prior id="ClockPrior" name="distribution" x="@clockRate">
                      <Uniform id="Uniform.0" name="distr" upper="Infinity"/>
                  </prior>
            </distribution>
            <!-- End prior -->


            <!-- Begin likelihood -->
            <distribution id="likelihood" spec="util.CompoundDistribution" useThreads="true">

                  <!-- C1 alignment likelihood -->
                  <distribution id="treeLikelihood.Dengue4.C1" spec="ThreadedTreeLikelihood" data="@Dengue4.C1" tree="@Tree.C1">
                      <siteModel id="SiteModel.C1" spec="SiteModel">
                          <parameter id="mutationRate.C1" estimate="false" name="mutationRate">1.0</parameter>
                          <parameter id="gammaShape.C1" estimate="false" name="shape">1.0</parameter>
                          <parameter id="proportionInvariant.C1" estimate="false" lower="0.0" name="proportionInvariant" upper="1.0">0.0</parameter>
                          <substModel id="hky" spec="HKY" kappa="@kappa">
                              <frequencies id="estimatedFreqs" spec="Frequencies" frequencies="@freqParameter"/>
                          </substModel>
                      </siteModel>
                      <branchRateModel id="StrictClock" spec="beast.evolution.branchratemodel.StrictClockModel" clock.rate="@clockRate"/>
                  </distribution>

                  <!-- C2 alignment likelihood -->
                  <distribution id="treeLikelihood.Dengue4.C2" spec="ThreadedTreeLikelihood" data="@Dengue4.C2" tree="@Tree.C2"              branchRateModel="@StrictClock">
                      <siteModel id="SiteModel.C2" spec="SiteModel" substModel="@hky">
                          <parameter id="mutationRate.C2" estimate="false" name="mutationRate">1.0</parameter>
                          <parameter id="gammaShape.C2" estimate="false" name="shape">1.0</parameter>
                          <parameter id="proportionInvariant.C2" estimate="false" lower="0.0" name="proportionInvariant" upper="1.0">0.0</parameter>
                      </siteModel>
                  </distribution>

            </distribution>
            <!-- End likelihood -->

      </distribution>


      <!-- Begin operators -->

            <!-- Site model operators -->
            <operator id="KappaScaler" spec="ScaleOperator" parameter="@kappa" scaleFactor="0.5" weight="0.1"/>
            <operator id="FrequenciesExchanger" spec="DeltaExchangeOperator" delta="0.01" weight="0.1">
                  <parameter idref="freqParameter"/>
            </operator>

            <!-- Clock model operators -->
            <operator id="StrictClockRateScaler" spec="ScaleOperator" parameter="@clockRate" scaleFactor="0.75" weight="3.0"/>
            <operator id="strictClockUpDownOperator.C1" spec="UpDownOperator" scaleFactor="0.75" weight="3.0">
                  <up idref="clockRate"/>
                  <down idref="Tree.C1"/>
            </operator>
            <operator id="strictClockUpDownOperator.C2" spec="UpDownOperator" scaleFactor="0.75" weight="3.0">
                  <up idref="clockRate"/>
                  <down idref="Tree.C2"/>
            </operator>

            <!-- C1 tree operators -->
            <operator id="BDSKY_serialtreeScaler.C1" spec="ScaleOperator" scaleFactor="0.5" tree="@Tree.C1" weight="3.0"/>
            <operator id="BDSKY_serialtreeRootScaler.C1" spec="ScaleOperator" rootOnly="true" scaleFactor="0.5" tree="@Tree.C1" weight="3.0"/>
            <operator id="BDSKY_serialUniformOperator.C1" spec="Uniform" tree="@Tree.C1" weight="30.0"/>
            <operator id="BDSKY_serialSubtreeSlide.C1" spec="SubtreeSlide" tree="@Tree.C1" weight="15.0"/>
            <operator id="BDSKY_serialnarrow.C1" spec="Exchange" tree="@Tree.C1" weight="15.0"/>
            <operator id="BDSKY_serialwide.C1" spec="Exchange" isNarrow="false" tree="@Tree.C1" weight="3.0"/>
            <operator id="BDSKY_serialWilsonBalding.C1" spec="WilsonBalding" tree="@Tree.C1" weight="3.0"/>

            <!-- C2 tree operators -->
            <operator id="BDSKY_serialtreeScaler.C2" spec="ScaleOperator" scaleFactor="0.5" tree="@Tree.C2" weight="3.0"/>
            <operator id="BDSKY_serialtreeRootScaler.C2" spec="ScaleOperator" rootOnly="true" scaleFactor="0.5" tree="@Tree.C2" weight="3.0"/>
            <operator id="BDSKY_serialUniformOperator.C2" spec="Uniform" tree="@Tree.C2" weight="30.0"/>
            <operator id="BDSKY_serialSubtreeSlide.C2" spec="SubtreeSlide" tree="@Tree.C2" weight="15.0"/>
            <operator id="BDSKY_serialnarrow.C2" spec="Exchange" tree="@Tree.C1" weight="15.0"/>
            <operator id="BDSKY_serialwide.C2" spec="Exchange" isNarrow="false" tree="@Tree.C2" weight="3.0"/>
            <operator id="BDSKY_serialWilsonBalding.C2" spec="WilsonBalding" tree="@Tree.C2" weight="3.0"/>

            <!-- BDSKY operators -->
            <operator id="becomeUninfectiousRateScaler" spec="ScaleOperator" parameter="@becomeUninfectiousRate" scaleFactor="0.75" weight="2.0"/>

                  <!-- C1 -->
                  <operator id="samplingScaler.C1" spec="ScaleOperator" parameter="@samplingProportion.C1" scaleFactor="0.75" weight="2.0"/>
                  <operator id="reproductiveNumberScaler.C1" spec="ScaleOperator" parameter="@reproductiveNumber.C1" scaleFactor="0.75" weight="10.0"/>
                  <operator id="origScaler.C1" spec="ScaleOperator" parameter="@origin.C1" scaleFactor="0.75" weight="1.0"/>
                  <operator id="updownBD.C1" spec="UpDownOperator" scaleFactor="0.75" weight="2.0">
                        <up idref="reproductiveNumber.C1"/>
                        <down idref="becomeUninfectiousRate"/>
                  </operator>

                  <!-- C2 -->
                  <operator id="samplingScaler.C2" spec="ScaleOperator" parameter="@samplingProportion.C2" scaleFactor="0.75" weight="2.0"/>
                  <operator id="reproductiveNumberScaler.C2" spec="ScaleOperator" parameter="@reproductiveNumber.C2" scaleFactor="0.75" weight="10.0"/>
                  <operator id="origScaler.C2" spec="ScaleOperator" parameter="@origin.C2" scaleFactor="0.75" weight="1.0"/>
                  <operator id="updownBD.C2" spec="UpDownOperator" scaleFactor="0.75" weight="2.0">
                        <up idref="reproductiveNumber.C2"/>
                        <down idref="becomeUninfectiousRate"/>
                  </operator>

      <!-- End operators -->



      <logger id="tracelog" fileName="dengue4_bdsky_equidistant5_treeslicer_hierarchical.log" logEvery="10000" model="@posterior" sanitiseHeaders="true" sort="smart">
            <log idref="posterior"/>
            <log idref="likelihood"/>
            <log idref="prior"/>
            <log idref="treeLikelihood.Dengue4.C1"/>
            <log idref="treeLikelihood.Dengue4.C2"/>
            <log idref="BirthDeathSkySerial.C1"/>
            <log idref="BirthDeathSkySerial.C2"/>
            <log idref="kappa"/>
            <log idref="freqParameter"/>
            <log idref="clockRate"/>
            
            <log id="TreeHeight.C1" spec="beast.evolution.tree.TreeHeightLogger" tree="@Tree.C1"/>
            <log id="TreeHeight.C2" spec="beast.evolution.tree.TreeHeightLogger" tree="@Tree.C2"/>
            <log spec="TreeDateLogger" tree="@Tree.C1"/>
            <log spec="TreeDateLogger" tree="@Tree.C2"/>            

            <log idref="origin.C1"/>
            <log spec="TreeDateLogger" tree="@Tree.C1" parameter="@origin.C1"/>
            <log idref="origin.C2"/>
            <log spec="TreeDateLogger" tree="@Tree.C2" parameter="@origin.C2"/>

            <log idref="reproductiveNumber.C1"/>
            <log idref="samplingProportion.C1"/>
            <log idref="ReTreeSlice.C1"/>            
            <log id="ReSliceDates.C1" spec="TreeSliceDateLogger" treeSlice="@ReTreeSlice.C1"/>

            <log idref="reproductiveNumber.C2"/>
            <log idref="samplingProportion.C2"/>
            <log idref="ReTreeSlice.C2"/>
            <log id="ReSliceDates.C2" spec="TreeSliceDateLogger" treeSlice="@ReTreeSlice.C2"/>

            <log idref="becomeUninfectiousRate"/>
      </logger>

      <logger id="screenlog" logEvery="10000">
            <log idref="posterior"/>
            <log id="ESS.0" spec="util.ESS" arg="@posterior"/>
            <log idref="likelihood"/>
            <log idref="prior"/>
      </logger>

      <logger id="treelog.C1" fileName="dengue4_bdsky_equidistant5_treeslicer_hierarchical.C1.trees" logEvery="10000" mode="tree">
            <log id="TreeWithMetaDataLogger.C1" spec="beast.evolution.tree.TreeWithMetaDataLogger" tree="@Tree.C1"/>
      </logger>

      <logger id="treelog.C2" fileName="dengue4_bdsky_equidistant5_treeslicer_hierarchical.C2.trees" logEvery="10000" mode="tree">
            <log id="TreeWithMetaDataLogger.C2" spec="beast.evolution.tree.TreeWithMetaDataLogger" tree="@Tree.C2"/>
      </logger>

</run>

</beast>
